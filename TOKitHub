getgenv().SecureMode = true
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Transfur Outbreak Exploiting Kit",
   LoadingTitle = "T.O Exploiting Kit",
   LoadingSubtitle = "by Mike",
   ConfigurationSaving = {
      Enabled = false,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "TO exploit kit"
   },
   Discord = {
      Enabled = true,
      Invite = "AkJNXt8KW9", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "autograb key",
      Subtitle = "Key System",
      Note = "this is exclusive software. ask Mike for the key.",
      FileName = "theSecretcode", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = false, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"https://raw.githubusercontent.com/profileaccount/scripts/main/keyKIT"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

Rayfield:Notify({
   Title = "Welcome!",
   Content = "Welcome to T.O Exploiting Kit, made by Mike.",
   Duration = 3,
   Image = 4483362458,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "Nice!",
         Callback = function()
         print("secretmessage69")
      end
   },
},
})

local Tab = Window:CreateTab("Essentials") -- Title, Image

local Section = Tab:CreateSection("Primary")

local Button = Tab:CreateButton({
   Name = "RealZzHub",
   Callback = function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/RealZzHub/MainV2/main/Main.lua"))()
	Rayfield:Notify({
   Title = "Executing...",
   Content = "Executing RealZzHub",
   Duration = 1,
   Image = 4483362458,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "Ok!",
         Callback = function()
         print("The user tapped Okay!")
      end
   },
},
})
   end,
})

