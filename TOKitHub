 })

local Tab = Window:CreateTab("Player")

local Section = Tab:CreateSection("Server Side")

local Paragraph = Tab:CreateParagraph({Title = "Note", Content = "RealZzHub includes all serversided related settings. Mike will never add server sided elements unless they dont exist currently in RealZzhub. Execute RealZzHub in the 'Essentials' tab! "})

local Section = Tab:CreateSection("Client Side")

local Input = Tab:CreateInput({
    Name = "Game Message",
    PlaceholderText = "input text here",
    RemoveTextAfterFocusLost = true,
    Callback = function(Text)
        game.ReplicatedStorage.GameMessage.Value = Text
    end,
 })

local ColorPicker = Tab:CreateColorPicker({
    Name = "Bullet Color",
    Color = Color3.fromRGB(255,255,255),
    Flag = "ColorPicker1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        local user = game.Players.LocalPlayer
        
        user.BulletColor.Value = Value
    end
})

local ColorPicker = Tab:CreateColorPicker({
    Name = "Chat Box Color",
    Color = Color3.fromRGB(255,255,255),
    Flag = "ColorPicker1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        local user1 = game.Players.LocalPlayer
        
        user1.BChatColor.Value = Value
    end
})

local ColorPicker = Tab:CreateColorPicker({
    Name = "Armor Color",
    Color = Color3.fromRGB(255,255,255),
    Flag = "ColorPicker1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        local user69 = game.Players.LocalPlayer.Character
        
        user69.ArmorModel.Torso.ATransparentPartLol.Color = Value
    end
})

